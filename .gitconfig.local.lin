[credential]
	helper = /usr/local/share/gcm-core/git-credential-manager-core
	credentialStore = gpg
[credential "https://dev.azure.com"]
	useHttpPath = true
[core]
	editor            = vim
[color]
	ui                = auto
[merge]
	tool              = vimdiff
[diff]
	tool              = vimdiff
[filter "media"]
	clean             = git-media-clean %f
	smudge            = git-media-smudge %f
[receive]
	denyCurrentBranch = ignore
