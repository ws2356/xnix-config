[credential]
	helper = osxkeychain
[core]
	editor            = vim
[color]
	ui                = auto
[merge]
	tool              = vimdiff
[diff]
	tool              = vimdiff
[filter "media"]
	clean             = git-media-clean %f
	smudge            = git-media-smudge %f
[receive]
	denyCurrentBranch = ignore
